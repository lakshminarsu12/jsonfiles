{
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "type": "AdaptiveCard",
    "version": "1.3",
    "body": [
      {
        "type": "Image",
        "url": "data:image/png;base64,[BASE64_ENCODED_IMAGE]",
        "altText": "ONT outlet placeholder",
        "size": "Medium",
        "horizontalAlignment": "Center"
      },
      {
        "type": "TextBlock",
        "text": "Let's find your ONT outlet",
